{
  "name": "muhammadsiyab/editorjs-parser-php",
  "description": "PHP parser for editorjs library",
  "keywords": [
    "editor.js",
    "html",
    "php",
    "parser"
  ],
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "MuhammadSiyab\\EditorjsParserPhp\\": "src/",
      "MuhammadSiyab\\EditorjsParserPhp\\Exceptions\\": "src/Exceptions/"
    }
  },
  "autoload-dev": {
    "MuhammadSiyab\\Tests\\": "tests/"
  },
  "authors": [
    {
      "name": "Muhammad Siyab",
      "email": "siyab.dev@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
